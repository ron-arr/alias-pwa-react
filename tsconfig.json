{
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "outDir": "./dist/",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "diagnostics": true,
        "noUnusedLocals": false,
        "removeComments": true,
        "target": "es5",
        "sourceMap": true,
        "strict": true,
        "esModuleInterop": true,
        "baseUrl": ".",
        "paths": {
            "als-pages/*": ["./src/app/pages/*"],
            "als-components/*": ["./src/app/components/*"],
            "als-models": ["./src/app/models/index.ts"],
            "als-data-types/*": ["src/app/dataTypes/*"],
            "als-ui": ["./src/app/ui/index.ts"],
            "als-ui/*": ["./src/app/ui/*"],
            "als-routes/*": ["./src/app/routes/*"],
            "als-services": ["./src/services/index.ts"],
            "als-services/*": ["./src/services/*"],
            "als-icons/*": ["src/jsxIcons/*"],
            "styles/*": ["./src/assets/styles/*"],
            "als-db-manager": ["als-db-manager/index.ts"]
        }
    },
    "include": ["src", "index.tsx"],
    "exclude": ["node_modules"],
    "files": ["./declarations.d.ts"]
}
