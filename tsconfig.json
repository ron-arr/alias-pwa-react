{
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "outDir": "./dist/",
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "diagnostics": true,
        "noUnusedLocals": true,
        "removeComments": true,
        "target": "es5",
        "sourceMap": true,
        "strict": true,
        "baseUrl": ".",
        "paths": {
            "als-pages/*": ["./src/app/pages/*"],
            "als-components/*": ["./src/app/components/*"],
            "als-models/*": ["./src/app/models/*"],
            "als-ui": ["./src/app/ui/index.ts"],
            "als-ui/*": ["./src/app/ui/*"],
            "als-routes/*": ["./src/app/routes/*"],
            "als-services": ["./src/services/index.ts"],
            "als-services/*": ["./src/services/*"],
            "als-icons/*": ["./src/icons/*"],
            "styles/*": ["./src/assets/styles/*"]
        }
    },
    "include": ["src", "index.tsx"],
    "exclude": ["node_modules"]
}
