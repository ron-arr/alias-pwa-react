{
    "version": 2,
    "name": "alias-demo",
    "alias": "alias-demo",
    "routes": [
        {
            "src": "^/assets/(.*)",
            "dest": "/public/assets/$1"
        },
        {
            "src": "^/favicon.svg",
            "dest": "/public/assets/favicon.svg"
        },
        {
            "src": "^/manifest.json",
            "dest": "/public/assets/manifest.json"
        },
        {
            "src": "^/(.*)",
            "dest": "/public/index.html"
        }
    ],
    "github": {
        "enabled": false,
        "autoAlias": false,
        "silent": true
    }
}