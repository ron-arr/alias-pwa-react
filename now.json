{
    "version": 2,
    "name": "alias-demo",
    "alias": "alias-demo",
    "routes": [
        {
            "src": "^/assets/(.*)",
            "dest": "/public/assets/$1"
        },
        {
            "src": "^/manifest.(.*)",
            "dest": "/public/manifest.$1"
        },
        {
            "src": "^/favicon.ico",
            "dest": "/public/favicon.ico"
        },
        {
            "src": "^/(.*)",
            "dest": "/public/index.html"
        }
    ],
    "github": {
        "enabled": false,
        "autoAlias": false,
        "silent": true
    }
}